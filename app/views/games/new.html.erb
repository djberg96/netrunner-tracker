<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <ul><label>Record Game</label></ul>
      <li class="first"><%= link_to "#{t("web-app-theme.list", :default => "List")}", games_path  %></li>
      <li class="active"><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_game_path %></li>
    </ul>
  </div>          
  <div class="content">            
    <div class="inner">
      <%= form_for :game, :url => games_path, :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
